[program:www-main]
user=vagrant
directory={{ work_dir }}/venv/
environment=PATH="{{ work_dir }}/venv/bin"
command={{ work_dir }}/www/server.py --app=main
autostart=true
autorestart=true
redirect_stderr=true
stdout_logfile={{ work_dir }}/www/log/tornado.log
stderr_logfile={{ work_dir }}/www/log/tornado.log
stopsignal=TERM
